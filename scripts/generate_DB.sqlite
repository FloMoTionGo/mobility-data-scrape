-- generate VBB Table
CREATE TABLE IF NOT EXISTS vbb (
   id varchar(255),
   created_at TIMESTAMP,
   type varchar(255),
   latitude varchar(255),
   longitude varchar(255)
   );

CREATE INDEX IF NOT EXISTS i_created_at
   ON vbb(created_at);

-- generate TIER table
CREATE TABLE IF NOT EXISTS TIER (
   id varchar(255),
   created_at TIMESTAMP,
   type varchar(255),
   latitude varchar(255),
   longitude varchar(255)
   );
   CREATE INDEX IF NOT EXISTS i_created_at
      ON TIER(created_at);
